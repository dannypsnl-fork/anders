def id : Π (A : U), A -> A :=
λ A, λ a, a

def sigmaIntro : Π (A : U), Π (B : A -> U),
                 Π (x : A), B x -> Σ (x : A), B x :=
λ A, λ B, λ x, λ u, (x, u)

def sigmaProj1 : Π (A : U), Π (B : A -> U),
                 Π (z : Σ (x : A), B x), B z.1 :=
λ A, λ B, λ z, z.2

def const : Π (A : U), Π (B : U), A -> B -> A :=
λ A, λ B, λ a, λ b, a