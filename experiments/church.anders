module church where

N := (A : U) -> (A -> A) -> A -> A
N0 : N := \(A : U) (S : A -> A) (Z : A) -> Z
succ : N -> N :=
\(n : N) (A : U) (S : A -> A) (Z : A) -> S (n A S Z)

plus (m n : N) : N :=
\(A : U) (S : A -> A) (Z : A) -> m A S (n A S Z)

mult (m n : N) : N :=
\(A : U) (S : A -> A) (Z : A) -> m A (n A S) Z

pow (m n : N) : N :=
\(A : U) (S : A -> A) (Z : A) -> n (A -> A) (m A) S Z